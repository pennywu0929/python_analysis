   "source": [
    "#如何依順序取出串列資料和映射資料的元素值\n",
    "#for_in迴圈\n",
    "fruits=['蘋果','香蕉','水梨']\n",
    "for fruit in fruits:print(fruit)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
