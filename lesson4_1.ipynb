      "0->蘋果\n",
      "=====\n",
      "1->香蕉\n",
      "=====\n",
      "2->水梨\n",
      "=====\n"
     ]
    }
   ],
   "source": [
    "for index,fruit in enumerate(fruits):\n",
    "    print(index,fruit,sep='->') #這裡的()是tuple資料\n",
    "    print(\"=====\")"
   ]
  }
